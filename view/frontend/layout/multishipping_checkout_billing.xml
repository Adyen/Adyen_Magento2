<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Adyen_Payment::css/adyen.css" />
    </head>
    <body>
        <referenceBlock name="checkout_billing" template="Adyen_Payment::checkout/multishipping/billing.phtml">
            <arguments>
                <argument name="checkout_data" xsi:type="object">Magento\Multishipping\Block\DataProviders\Billing</argument>
                <argument name="form_templates" xsi:type="array">
                    <item name="adyen_cc" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_klarna" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_ideal" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_bcmc_mobile" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_dotpay" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_multibanco" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_sepadirectdebit" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_affirm" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_afterpaytouch" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_alipay" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_alipay_hk" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_alma" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_atome" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_afterpay_default" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_bankTransfer_BE" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                    <item name="adyen_bankTransfer_CH" xsi:type="string">Adyen_Payment::form/multishipping/abstract-form.phtml</item>
                </argument>
                <argument name="cacheable" xsi:type="boolean">false</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>